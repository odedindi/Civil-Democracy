{
	"name": "civil-democracy",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"check:format": "prettier --check .",
		"check:types": "tsc --pretty --noEmit",
		"format": "prettier --write .",
		"prisma:generate": "prisma generate",
		"prisma:migrate:dev": "prisma migrate dev && yarn prisma-uml prisma/schema.prisma --output svg --file ./db.svg",
		"prisma:studio": "prisma studio",
		"seed": "ts-node-dev --transpile-only prisma/seed.ts",
		"apollo:generate": "turbo run generate --filter=apollo-hooks",
		"prepare": "husky install"
	},
	"prisma": {
		"seed": "ts-node-dev --transpile-only prisma/seed.ts"
	},
	"dependencies": {
		"@apollo/client": "^3.7.14",
